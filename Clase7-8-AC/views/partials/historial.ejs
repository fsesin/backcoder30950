<h1 style="color: red">historial</h1>
<% if(personas.length > 0) {%>
<table class="table">
  <tr style="color: black">
    <th>Nombre</th>
    <th>Apellido</th>
    <th>Edad</th>
  </tr>
  <% personas.forEach(persona=>{ %>
  <tr>
    <td><%= persona.nombre %></td>
    <td><%= persona.apellido %></td>
    <td><%= persona.edad %></td>
  </tr>
  <% }) %>
</table>
<% } else { %>
<h2 style="color: blueviolet">No se encontraron datos</h2>
<% } %>
